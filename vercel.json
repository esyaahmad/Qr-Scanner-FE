{
    "version": 2,
    "public": true,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/${NAMA_PROGRAM}/(.*)",
        "dest": "/${NAMA_PROGRAM}/index.html"
      }
    ]
  }
  